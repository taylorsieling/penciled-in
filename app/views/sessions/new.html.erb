<p><%= flash[:message] if flash[:message] %></p>
<p> <% if @errors %>
<%= @erros %>
<% end %></p>
  
<h1>Login</h1>
<%= form_for :user do |f| %>
<%= f.label :email %>
<%= f.email_field :email %> <br>
<%= f.label :password %>
<%= f.password_field :password %> <br>
<%= f.submit "Log in" %>
<% end %>

<h3> Or </h3>

<%= button_to "Log in with Google", '/auth/google_oauth2' %>